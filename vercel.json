{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb-uri",
    "DB_NAME": "@db-name",
    "FIREBASE_PROJECT_ID": "@firebase-project-id"
  }
}